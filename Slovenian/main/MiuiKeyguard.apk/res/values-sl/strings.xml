  <string name="phone_locked_foget_password_method_content">Morate &lt;font color="#a5220e"&gt;izbrisati vse podatke&lt;/font&gt; v vaši napravi, vključno fotografije in stike, potem pa nastavite novo geslo zakl. zaslona.</string>
  <string name="phone_locked_foget_password_method_next">Naprej</string>
  <string name="phone_locked_foget_password_ok">Razumem</string>
  <string name="phone_locked_forget_password_suggesstion_one_content">Izključite napravo, pritisnite in držite &lt;font color="#a5220e"&gt;Tipko za vklop&lt;/font&gt; in &lt;font color="#a5220e"&gt;Glasnost +&lt;/font&gt;istočasno dokler ne vidite MI logo.</string>
  <string name="phone_locked_forget_password_suggesstion_two_content">Izberite &lt;font color="#a5220e"&gt;\"Izbriši &amp;amp; obnovi\"&lt;/font&gt; in počakajte da se naprava ponovno zažene.</string>
